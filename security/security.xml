<?xml version='1.0' encoding='utf-8'?>
<odoo>
    
    <data noupdate="1">
    
        <record id="module_category_repair_request" model="ir.module.category">
            <field name="name">Repair Request</field>
        </record>
        
        <record id="group_repair_user" model="res.groups">
            <field name="name">Repair User</field>
            <field name="category_id" ref="module_category_repair_request"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="users" eval="[(5, ref('base.user_root'))]" />
        </record>
        
        <record id="group_repair_manager" model="res.groups">
            <field name="name">Repair Manager</field>
            <field name="category_id" ref="module_category_repair_request"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user')), (4, ref('group_repair_user'))]"/>
            <field name="users" eval="[(5, ref('base.user_root'))]" />
        </record>
    
    </data>
    
</odoo>